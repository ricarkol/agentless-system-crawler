#!/bin/bash

declare -A DOCKER_DEVICES=(
  [$ES1]="/dev/xvdc" 
  )
DOCKER_PARTITION_START=1MB
DOCKER_PARTITION_END=30GB
DOCKER_PARTITION_FSTYPE=btrfs
DOCKER_PARTITION_MOUNTPOINT=/var/lib/docker

declare -A DOCKER_DEVICES_LARGE=(
  )
DOCKER_PARTITION_LARGE_END=-1

DATA_PARTITION_START=30GB
DATA_PARTITION_END=-1
DATA_PARTITION_FSTYPE=ext4
DATA_PARTITION_MOUNTPOINT=/mnt/data
declare -A DATA_DEVICES=(
  [$ES1]="/dev/xvdc" 
  )
